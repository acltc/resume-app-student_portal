	

	<%= form_tag "/educations/#{@education['id']}", :method => :patch do %>

	<%= label_tag :university_name %>
	<%= text_field_tag :university_name, @education['university_name'] %>

	<%= label_tag :degrees %>
	<%= text_field_tag :degrees, @education['degrees'] %>

	<%= label_tag :start_date %>
	<%= text_field_tag :start_date, @education['start_date'] %>

	<%= label_tag :end_date %>
	<%= text_field_tag :end_date, @education['end_date']%>

	<%= submit_tag "update" %>
	<% end %>


